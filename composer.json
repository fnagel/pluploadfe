{
	"name": "felixnagel/pluploadfe",
	"type": "typo3-cms-extension",
	"description": "This TYPO3 CMS extension provides an API and a FE plugin for using plupload, a highly usable and advanced upload handler.",
	"keywords": [
		"TYPO3 CMS",
		"upload",
		"plupload",
		"pluploadfe"
	],
	"homepage": "https://extensions.typo3.org/extension/pluploadfe/",
	"support": {
		"issues": "https://github.com/fnagel/pluploadfe/issues",
		"docs": "https://docs.typo3.org/p/felixnagel/pluploadfe/main/en-us",
		"source": "https://github.com/fnagel/pluploadfe"
	},
	"authors": [
		{
			"name": "Felix Nagel",
			"email": "info@felixnagel.com",
			"homepage": "https://www.felixnagel.com",
			"role": "Developer"
		}
	],
	"license": "GPL-2.0-or-later",
	"require": {
		"php": "^8.2 || ^8.3",
		"ext-json": "*",
		"typo3/cms-core": "^12.4 || ^13"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^3.12",
		"ssch/typo3-rector": "^2.3"
	},
	"suggest": {
		"ext-fileinfo": "Helpful for file Mime type detection",
		"felixnagel/mailfiles": "Simple message form with file upload",
		"felixnagel/plupload_powermail": "Powermail integration"
	},
	"autoload": {
		"psr-4": {
			"FelixNagel\\Pluploadfe\\": "Classes/"
		}
	},
	"replace": {
		"typo3-ter/pluploadfe": "self.version"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/TYPO3/TYPO3.CMS"
		}
	],
	"extra": {
		"typo3/cms": {
			"extension-key": "pluploadfe"
		}
	}
}
